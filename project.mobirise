ition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n            <div class=\"col-12 col-lg-7\">\n                <div class=\"logo-wrapper\" mbr-if=\"showLogo\">\n                    <a href=\"#\"><img src=\"@PROJECT_PATH@/assets/images/logo-ccecc-txt-512-min-246x144.png\" alt=\"CCECC\"></a>\n                </div>\n            </div>\n            <div class=\"col-lg-4\">\n                <div class=\"content-wrapper\" mbr-if=\"showContacts || showSoc\">\n                    <div class=\"list-wrapper\" mbr-if=\"showSoc\">\n                        <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list, .item-wrap, .list-wrapper\" data-multiline mbr-article>\n                            <li class=\"item-wrap\">Mapa del sitio</li>\n                            <li class=\"item-wrap\">Términos y condiciones</li>\n                            <li class=\"item-wrap\">Políticas</li>\n                        </ul>\n                    </div>\n                    <div class=\"list-wrapper\" mbr-if=\"showContacts\">\n                        <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list, .item-wrap, .list-wrapper\" data-multiline mbr-article>\n                            <li class=\"item-wrap\">presiden